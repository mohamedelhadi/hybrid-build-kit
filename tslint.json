{
    "extends": [
        "tslint-eslint-rules",
        "tslint-config-airbnb"
    ],
    "rules": {
        "no-param-reassign": false,
        "ter-indent": [true, 4, {
            "SwitchCase": 1
        }],
        "no-increment-decrement": false,
        "ter-arrow-parens": false,
        "member-ordering": [
            true
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "arrow-return-shorthand": [
            true
        ],
        "unified-signatures": true,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ]
    }
}